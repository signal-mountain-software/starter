{
  "name": "starter",
  "description": "A simple starter for Signal Mountain Software applications to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": {
    "name": "Jalo Moster",
    "email": "jmoster@signalmountainsoftware.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/signal-mountain-software/starter"
  },
  "bugs": {
    "url": "https://github.com/signal-mountain-software/starter/issues"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "0BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,css,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@headlessui/react": "^1.0.0",
    "@heroicons/react": "^1.0.1",
    "@tailwindcss/forms": "^0.3.2",
    "aws-amplify": "^3.3.27",
    "clsx": "^1.1.1",
    "gatsby": "^3.3.1",
    "gatsby-plugin-image": "^1.1.1",
    "gatsby-plugin-manifest": "^3.3.0",
    "gatsby-plugin-offline": "^4.1.0",
    "gatsby-plugin-react-helmet": "^4.1.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sharp": "^3.3.1",
    "gatsby-plugin-sitemap": "^3.1.0",
    "gatsby-source-filesystem": "^3.1.0",
    "gatsby-transformer-sharp": "^3.3.0",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "recoil": "^0.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.5",
    "gatsby-plugin-postcss": "^4.3.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.12",
    "prettier": "2.2.1",
    "prettier-plugin-tailwind": "^2.2.10",
    "tailwindcss": "^2.1.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,json,css,md}": [
      "prettier --write"
    ]
  }
}
